pipeline {
	agent {
		docker {
			image 'ubuntu:v1_m'
		}
	}
	stages {
		stage('Build') {
			steps {
				sh '''
				cd $WORKSPACE
				python2 -V
				python2 example.py
				echo "hello world"
				hostname
				pwd 
				ls'''
				
			}
		}
	}
	post {
		success {
			withCredentials([gitUsernamePassword(credentialsId: 'maheedhar',
                 gitToolName: 'git-tool')]) {
				sh '''
				echo GIT_BRANCH
				git branch -a
				git checkout main
				git checkout integrate -- example.py
				git commit -m "Pushig file from integrate to main."
				git push origin main'''
			}
		}
	}
}
