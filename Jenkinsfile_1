pipeline {
	agent {
		docker {
			image 'ubuntu:v1_m'
		}
	}
	stages {
		stage('Build') {
			steps {
				sh '''
				cd $WORKSPACE
				python2 -V
				python2 example.py
				echo "hello world"
				hostname
				pwd 
				ls'''
				
			}
		}
	}
	post {
		success {
			withCredentials([gitUsernamePassword(credentialsId: 'maheedhar', gitToolName: 'git-tool')]) {
			sh '''
			export https_proxy="http://proxy-chain.intel.com:912"
			git config user.name $username
			git config user.email $usermail
			echo $GIT_BRANCH
			git fetch origin
			git checkout main
			git merge origin/integrate
			git push origin main
			'''
			
		}}
	}
}
