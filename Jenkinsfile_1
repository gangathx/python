pipeline {
	agent {
		docker {
			image 'ubuntu:v1_m'
		}
	}
	stages {
		stage('Build') {
			steps {
				sh '''
				cd $WORKSPACE
				python2 -V
				python2 example.py
				echo "hello world"
				hostname
				pwd 
				ls'''
				
			}
		}
	}
	post {
		success {
			withCredentials([usernamePassword(credentialsId: 'maheedhar', passwordVariable: 'password', usernameVariable: 'username')]){
			sh '''
			export https_proxy="http://proxy-chain.intel.com:912"
			git config user.name $username
			git config user.email $usermail
			echo $GIT_BRANCH
			git branch -a
			git checkout main
			git checkout origin/integrate -- example.py
			git status
			git commit -m "pushing file from integrate to master"
			git push https://${username}:${password}@$url'''
			
		}}
	}
}
